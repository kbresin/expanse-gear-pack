{"_id":"Q7nElNfhIBJ0UM9q","name":"RCE Light Shuttle","permission":{"default":0,"wypGoDpbSltPFEBU":3},"type":"spaceship","data":{"size":"2","hull":{"nrDice":1,"diceType":6,"extraValue":0},"drive":"","description":"<p class=\"MsoNormal\" style=\"margin: 0in; font-size: 12pt; font-family: Calibri, sans-serif;\"><span style=\"font-size: 10pt;\">The RCE light shuttle is a smaller shuttle also used by Royal Charter Energy.</span></p>\n<p class=\"MsoNormal\" style=\"margin: 0in; font-size: 12pt; font-family: Calibri, sans-serif;\"><span style=\"font-size: 10pt;\">The Edward Israel carried two of these during its trip to Ilus IV. Both however were destroyed by over the planet, with one by the planet's moon, and the other being converted into a bomb to attack the Rocinante and Barbapiccola.</span></p>","passengers":[],"crew":{"min":1,"current":2,"typical":1,"competence":"2"},"weapons":{},"systems":{"maneuver":{"mod":0,"base":0,"operator":"crew","useAbl":"dex","useFocus":""},"command":{"mod":0,"base":0,"operator":"crew","useAbl":"comm","useFocus":""},"sensors":{"mod":0,"base":1,"operator":"crew","useAbl":"int","useFocus":""},"damageControl":{"mod":0,"base":0,"operator":"crew","TN":11,"useAbl":"int","useFocus":"","threshold":{"current":0,"target":5}}},"losses":{"normal":{"collateral":{"actual":0,"max":6},"hull":{"actual":0,"max":6},"maneuverability":{"actual":0,"max":6},"sensors":{"actual":0,"max":6},"weapons":{"actual":0,"max":6}},"serious":{"reactorOffline":{"actual":0,"max":6},"weaponOffline":{"actual":0,"max":6}}},"takenOut":false},"sort":100001,"flags":{},"img":"modules/expanse-pack/item-assets/ships/S04e06rcelightshuttle17m17s.jpeg","token":{"flags":{},"name":"Light Shuttle","displayName":50,"img":"modules/expanse-pack/token-assets/Expanse_Ships/Hybrid_Pod_5m_token.png","tint":"","width":2,"height":2,"scale":1,"mirrorX":false,"mirrorY":false,"lockRotation":false,"rotation":0,"vision":false,"dimSight":0,"brightSight":0,"dimLight":0,"brightLight":0,"sightAngle":360,"lightA