{"_id":"6yruBsiWAxlGFTBm","name":"MCRN Mosquito Breaching Pod","permission":{"default":0,"wypGoDpbSltPFEBU":3},"type":"spaceship","data":{"size":"1","hull":{"nrDice":1,"diceType":6,"extraValue":0},"drive":"","description":"<p class=\"MsoNormal\" style=\"margin: 0in; font-size: 12pt; font-family: Calibri, sans-serif;\"><span style=\"font-size: 10pt;\">The Mosquito type breaching pods are attached to Scirocco-class which carries 12 per ship (6 on each side). This pod is hexagonal shaped.</span></p>","passengers":[],"crew":{"min":1,"current":2,"typical":1,"competence":"2"},"weapons":{},"systems":{"maneuver":{"mod":0,"base":0,"operator":"crew","useAbl":"dex","useFocus":""},"command":{"mod":0,"base":0,"operator":"crew","useAbl":"comm","useFocus":""},"sensors":{"mod":0,"base":1,"operator":"crew","useAbl":"int","useFocus":""},"damageControl":{"mod":0,"base":0,"operator":"crew","TN":11,"useAbl":"int","useFocus":"","threshold":{"current":0,"target":5}}},"losses":{"normal":{"collateral":{"actual":0,"max":6},"hull":{"actual":0,"max":6},"maneuverability":{"actual":0,"max":6},"sensors":{"actual":0,"max":6},"weapons":{"actual":0,"max":6}},"serious":{"reactorOffline":{"actual":0,"max":6},"weaponOffline":{"actual":0,"max":6}}},"takenOut":false},"sort":100001,"flags":{},"img":"modules/expanse-pack/item-assets/ships/160202_NorthFront_ext_anubis_breaching_pod_01_cropped.jpg","token":{"flags":{},"name":"Mosquito Breaching Pod","displayName":50,"img":"modules/expanse-pack/token-assets/Expanse_Ships/Y_Que_Slingshot_10.3m_token.png","tint":"","width":1,"height":1,"scale":1,"mirrorX":false,"mirrorY":false,"lockRotation":false,"rotation":0,"vision":false,"dimSight":0,"brightSight":0,"dimLight":0,"brightLight":0,"sightAngle":360,"lightAngle":360,"lightColor":"","lightAlpha":1,"lightAnimation":{"type":"","speed":5,"intensity":5},"actorId":"6yruBsiWAxlGFTBm","actorLink":false,"disposition":-1,"displayBars":0,"bar1":{"attribute":""},"bar2":{"attribute":""},"randomImg":false},"items":[],"effects